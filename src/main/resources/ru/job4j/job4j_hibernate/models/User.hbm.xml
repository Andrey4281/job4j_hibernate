<?xml version = "1.0" encoding = "utf-8"?>

<hibernate-mapping>
    <class name = "ru.job4j.job4j_hibernate.models.User" table = "users">

        <id name = "id" column = "id">
            <generator class="identity"/>
        </id>

        <property name = "name" column = "name"/>
        <property name = "expired" column = "expired"/>

    </class>
</hibernate-mapping>